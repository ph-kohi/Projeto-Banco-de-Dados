<?xml version="1.0" encoding="UTF-8"?>
<project>
  <model type="logical">

    <table name="CLIENTE">
      <column name="ID_CLIENTE" type="INT" primaryKey="true" />
      <column name="NOME" type="VARCHAR(100)" />
      <column name="CPF" type="CHAR(11)" unique="true" />
      <column name="DATA_NASCIMENTO" type="DATE" />
      <column name="TELEFONE" type="VARCHAR(20)" />
    </table>

    <table name="DENTISTA">
      <column name="ID_DENTISTA" type="INT" primaryKey="true" />
      <column name="NOME" type="VARCHAR(100)" />
      <column name="CRO" type="VARCHAR(20)" unique="true" />
      <column name="TELEFONE" type="VARCHAR(20)" />
    </table>

    <table name="PROTETICO">
      <column name="ID_PROTETICO" type="INT" primaryKey="true" />
      <column name="NOME" type="VARCHAR(100)" />
      <column name="ESPECIALIDADE" type="VARCHAR(50)" />
    </table>

    <table name="PEDIDO">
      <column name="ID_PEDIDO" type="INT" primaryKey="true" />
      <column name="DATA_PEDIDO" type="DATE" />
      <column name="STATUS_PEDIDO" type="VARCHAR(30)" />

      <column name="ID_CLIENTE" type="INT" foreignKey="true" reference="CLIENTE.ID_CLIENTE" />
      <column name="ID_DENTISTA" type="INT" foreignKey="true" reference="DENTISTA.ID_DENTISTA" />
    </table>

    <table name="PROTESE">
      <column name="ID_PROTESE" type="INT" primaryKey="true" />
      <column name="TIPO" type="VARCHAR(50)" />
      <column name="MATERIAL" type="VARCHAR(50)" />
      <column name="COR" type="VARCHAR(30)" />

      <column name="ID_PEDIDO" type="INT" foreignKey="true" reference="PEDIDO.ID_PEDIDO" />
    </table>

    <table name="ETAPA_PRODUCAO">
      <column name="ID_ETAPA" type="INT" primaryKey="true" />
      <column name="NOME_ETAPA" type="VARCHAR(100)" />
      <column name="DATA_INICIO" type="DATE" />
      <column name="DATA_FIM" type="DATE" />
      <column name="STATUS_ETAPA" type="VARCHAR(30)" />

      <column name="ID_PEDIDO" type="INT" foreignKey="true" reference="PEDIDO.ID_PEDIDO" />
    </table>

    <table name="PEDIDO_PROTETICO">
      <column name="ID_PEDIDO" type="INT" foreignKey="true" reference="PEDIDO.ID_PEDIDO" />
      <column name="ID_PROTETICO" type="INT" foreignKey="true" reference="PROTETICO.ID_PROTETICO" />
      <column name="FUNCAO" type="VARCHAR(50)" />

      <primaryKey>
        <column name="ID_PEDIDO"/>
        <column name="ID_PROTETICO"/>
      </primaryKey>
    </table>

  </model>
</project>
